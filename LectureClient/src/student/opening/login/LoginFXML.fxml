<VBox fx:id="LoginPane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="395.0" prefWidth="300.0" spacing="30.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.opening.login.LoginFXMLController">
    <children>
        <JFXTextField fx:id="username" focusColor="#85c1e9" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" promptText="Username" style="-fx-text-fill: #85c1e9;" unFocusColor="#85c1e9" VBox.vgrow="ALWAYS">
            <font>
                <Font size="15.0" />
            </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
        </JFXTextField>
        <JFXPasswordField fx:id="password" focusColor="#85c1e9" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" promptText="Password" style="-fx-text-fill: #85c1e9;" unFocusColor="#85c1e9" VBox.vgrow="ALWAYS">
            <font>
                <Font size="15.0" />
            </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
        </JFXPasswordField>
        <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#LoginAction" style="-fx-background-color: #85C1E9;" text="Login" textFill="WHITE" VBox.vgrow="ALWAYS">
