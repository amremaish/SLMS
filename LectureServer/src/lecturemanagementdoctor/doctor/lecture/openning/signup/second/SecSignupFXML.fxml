<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="SecSignUpPane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="410.0" prefWidth="320.0" spacing="30.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lecturemanagementdoctor.doctor.lecture.openning.signup.second.SecSignupFXMLController"> 
    <children>
        <JFXTextField fx:id="phone" focusColor="#7ebde7" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" prefWidth="267.0" promptText="Phone number" style="-fx-text-fill: #85c1e9;" unFocusColor="#85c1e9">
            <font>
                <Font size="15.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </JFXTextField>
        <JFXTextField fx:id="department" focusColor="#7ebde7" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" prefWidth="267.0" promptText="Department" style="-fx-text-fill: #85c1e9;" unFocusColor="#85c1e9">
            <font>
                <Font size="15.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </JFXTextField>
        <JFXTextField fx:id="courseName" focusColor="#7ebde7" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" prefWidth="267.0" promptText="Course Name" style="-fx-text-fill: #85c1e9;" unFocusColor="#85c1e9">
            <font>
                <Font size="15.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </JFXTextField>
      <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="35.0" prefWidth="327.0">
