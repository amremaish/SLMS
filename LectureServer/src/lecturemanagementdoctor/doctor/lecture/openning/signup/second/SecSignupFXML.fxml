                  <Insets right="10.0" />
               </HBox.margin>
            </Text>
            <JFXRadioButton fx:id="male" text="Male" textFill="#85c1e9" toggleGroup="$gender">
               <font>
                  <Font size="14.0" />
               </font>
               <toggleGroup>
                  <ToggleGroup fx:id="gender" />
               </toggleGroup>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </JFXRadioButton>
            <JFXRadioButton fx:id="female" text="Female" textFill="#85c1e9" toggleGroup="$gender">
               <font>
                  <Font size="14.0" />
               </font>
            </JFXRadioButton>
         </children>
         <VBox.margin>
            <Insets bottom="-10.0" top="-10.0" />
         </VBox.margin>
      </HBox>
        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#SignUpAction" prefHeight="36.0" prefWidth="186.0" style="-fx-background-color: #85C1E9;" text="Sign Up" textFill="WHITE">
